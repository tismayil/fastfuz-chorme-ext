{
    "manifest_version": 2,
    "name": "FastFuzz CSP",
    "version": "1.0",
    "description": "FastFuzz fast fuzzing with chrome extension",
    "icons":{
        "16":"/images/16.png",
        "48":"/images/48.png",
        "128":"/images/128-1.png"
    },
    "browser_action":{
        "default_icon":"/images/48.png",
        "default_popup":"popup.html"
    },
    "permissions":[
        "tabs",  
        "http://*/*",
        "https://*/*"
    ]    
}